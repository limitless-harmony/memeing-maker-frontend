    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-easy-crop": "^1.9.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.8.0",
    "react-infinite-scroller": "^1.2.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "react-toastify": "^5.2.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
